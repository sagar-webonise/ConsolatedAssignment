            <script type="text/javascript" >
$(document).ready(function(){
 				$.ajax({   
                              type: "GET",
                                async:true,
                                dataType:'json',
                                url: "user.json",
                                complete: function(success,notify) {
                             
                                 },
                                 dataFilter:function(data){
	                                 obj = JSON.parse(data);
	                                 for(var i=0;i<obj.length;i++)
	                                   $('#data').append("<tr><td>"+obj[i].name+"</td><td>"+obj[i].email+"</td><td>"+obj[i].age+"</td><td>"+obj[i].gender+"</td><td>"+obj[i].address+"</td><td>"+obj[i].pincode+"</td><td>"+obj[i].description+"</td></tr>")
	                                   $('#data').append("</table>")
 												     },
                             		success:function(){
                                    
                                         }
        		});  
        
         });            
            
            

</script>

    </head>
    <body>
    
    
        
        <div class="form">
        <h2>DATA FROM SERVER:</h2>
